<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.DesignActivity"
        tools:targetApi="31">
        <activity
            android:name=".mvvm.activity.ApiCallingActivity"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".activity.ExitSplashScreenActivity"
            android:exported="false" />
        <activity
            android:name=".activity.ExitActivity"
            android:exported="false" />
        <activity
            android:name=".activity.MySavedDownloadsActivity"
            android:exported="false" />
        <activity
            android:name=".activity.AllAppActivty"
            android:exported="false"
            android:screenOrientation="locked" />
        <activity
            android:name=".activity.MediaDownloaderActivity"
            android:exported="false"
            android:screenOrientation="locked" />
        <activity
            android:name=".activity.AllVideoDownloaderActivity"
            android:exported="false"
            android:screenOrientation="locked" />
        <activity
            android:name=".activity.DashBordActivity"
            android:exported="false"
            android:screenOrientation="locked" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true"
            android:screenOrientation="locked" />

    </application>

</manifest>